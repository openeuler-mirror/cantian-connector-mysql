create table tbl_bit (num bit(64), index idx_num(num));
create procedure insert_nums_to_tbl_bit()
begin
declare i int default 1;
while i <= 10000 do
insert into tbl_bit (num) values (i);
set i = i + 1;
end while;
end//
call insert_nums_to_tbl_bit();
