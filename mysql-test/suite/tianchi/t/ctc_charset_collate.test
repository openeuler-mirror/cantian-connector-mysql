SELECT c1, HEX(c1), HEX(WEIGHT_STRING(c1)) FROM t1;
drop table t1;

# select - collate: weight_string
create table t1 (c1 varchar(5) collate utf8mb4_0900_ai_ci, key(c1));
create table t2 (c1 varchar(5) collate utf8mb4_bin, key(c1));
