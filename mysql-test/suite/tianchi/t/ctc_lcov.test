set global ctc_stats_enabled=ON;
show engine ctc status;
create table t1(i int);
insert into t1 value(123);
set debug = '+d, non_metadata_normalization';
lock table t1 write;
unlock tables;         
set debug = '-d, non_metadata_normalization';
set sort_buffer_size = (select i from t1);
