create table t1(a bigint , b char(255), c date) partition by list columns(a,b,c)
(partition p0 values in((4,'aaa', '1948-10-10'),(5,'abb', '1949-10-10'),
(6,'abc', '1950-10-10'),(7,'abd', '1951-10-10')),
partition p1 values in((10, 'baa', '1990-10-10'),(11, 'bab', '1991-10-10'),
(11, 'bac', '1992-10-10'),(12, 'bad', '1993-10-10')),
partition p2 values in((20, 'caa', '2010-10-10'),(21, 'cab', '2011-10-10'),
(22, 'cac', '2012-10-10'),(23, 'cad', '2013-10-10')),
partition p3 values in ((30, 'daa', '2020-10-10'),(31, 'dab', '2021-10-10'),
(32, 'dac', '2022-10-10'),(33, 'dae', '2023-10-10')));
insert into t1 values(4,'aaa', '1948-10-10'),(10, 'baa', '1990-10-10'),
(32, 'dac', '2022-10-10'),(22, 'cac', '2012-10-10');
update t1 set a=6,b='abc',c='1950-10-10' where a=4;
update t1 set a=32,b='dac',c='2022-10-10' where a=6;
update t1 set a=11,b='bac',c='1992-10-10' where a=10;
update t1 set a=4,b='aaa',c='1948-10-10' where a=22;
drop table t1;
